        </activity>
        <activity
            android:name=".TablicaWpisow.TrescWpisuActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".TablicaWpisow.DodajWpisActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Portfel.PortfelActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Pozostale.AutorzyActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Pozostale.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".Portfel.WezDaneDoPortfela"></activity>
    </application>

</manifest>